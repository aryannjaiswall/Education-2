                            li
                                a(href='/') Home
                            li
                                a(href='/about') About
                            li
                                a(href='/courses') Courses
                            li
                                a(href='/instructions') Instructions
                            li
                                a(href='/blogs') Blog
                            li
                                a(href='/contact') Contact Us
                    .search-container
                        form(action='/')
                            input(type='text' placeholder='   Search Courses' name='search')
                            i#searchIcon.medium.material-icons search
        block content
        block foot
            footer
                .row-1
                    h1 Stay Updated
                    h1 Let's Get Social 
                .row-2
                    .row-2-input
                        input.footer-input(type='email' placeholder='Email Address...')
